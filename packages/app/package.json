{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@repo/app",
	"type": "module",
	"private": true,
	"imports": {
		"#root/*": "./*"
	},
	"dependencies": {
		"@conform-to/react": "1.2.2",
		"@effect/platform": "0.77.6",
		"@effect/platform-node": "0.73.6",
		"@epic-web/client-hints": "1.3.5",
		"@epic-web/invariant": "1.0.0",
		"@octokit/core": "6.1.4",
		"@octokit/plugin-rest-endpoint-methods": "13.3.1",
		"@octokit/request-error": "6.1.7",
		"@react-router/node": "7.2.0",
		"@repo/open-graph-protocol": "workspace:*",
		"@repo/tailwind-config": "workspace:*",
		"@repo/ui": "workspace:*",
		"@std/yaml": "npm:@jsr/std__yaml@1.0.5",
		"@suddenly-giovanni/schema-resume": "npm:@jsr/suddenly-giovanni__schema-resume@0.0.5",
		"cookie": "1.0.2",
		"effect": "3.13.2",
		"isbot": "5.1.23",
		"react": "catalog:",
		"react-dom": "catalog:",
		"react-router": "7.2.0",
		"remix-utils": "8.1.0"
	},
	"devDependencies": {
		"@babel/preset-typescript": "7.26.0",
		"@codecov/vite-plugin": "1.9.0",
		"@mdx-js/rollup": "3.1.0",
		"@react-router/dev": "7.2.0",
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@tailwindcss/vite": "catalog:",
		"@total-typescript/ts-reset": "catalog:",
		"@types/mdx": "2.0.13",
		"@types/node": "22.13.9",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitest/coverage-v8": "catalog:",
		"@vitest/eslint-plugin": "1.1.36",
		"@vitest/ui": "catalog:",
		"babel-plugin-react-compiler": "catalog:",
		"globals": "16.0.0",
		"msw": "2.7.3",
		"react-router-devtools": "2.0.0-beta.0",
		"tailwindcss": "catalog:",
		"vite": "catalog:",
		"vite-plugin-babel": "1.3.0",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "catalog:"
	},
	"scripts": {
		"build": "react-router build",
		"check": "biome check .",
		"check:fix": "biome check --apply .",
		"clean": "rm -rf node_modules build coverage pruned .react-router",
		"dev": "node --env-file=.development.env --experimental-network-inspection server/server.ts",
		"format": "biome check --vcs-enabled=true --vcs-use-ignore-file=true  --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=true .",
		"format:write": "biome check --vcs-enabled=true --vcs-use-ignore-file=true  --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=true --write .",
		"lint": "biome ci --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=false --linter-enabled=true --organize-imports-enabled=false --no-errors-on-unmatched .",
		"start": "node --env-file=.production.env --inspect server/server.ts",
		"test": "vitest",
		"typecheck": "react-router typegen && tsc --noEmit"
	},
	"sideEffects": false
}
