{
	"$schema": "https://json.schemastore.org/package.json",
	"dependencies": {
		"@conform-to/react": "catalog:",
		"@effect/platform": "catalog:",
		"@effect/platform-node": "catalog:",
		"@epic-web/client-hints": "catalog:",
		"@epic-web/invariant": "catalog:",
		"@octokit/core": "catalog:",
		"@octokit/plugin-rest-endpoint-methods": "catalog:",
		"@octokit/request-error": "catalog:",
		"@react-router/node": "catalog:",
		"@repo/open-graph-protocol": "workspace:*",
		"@repo/tailwind-config": "workspace:*",
		"@repo/ui": "workspace:*",
		"@std/yaml": "jsr:1.0.5",
		"@suddenly-giovanni/schema-resume": "jsr:0.0.7",
		"cookie": "catalog:",
		"effect": "catalog:",
		"isbot": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"react-router": "catalog:",
		"remix-utils": "catalog:"
	},
	"devDependencies": {
		"@babel/preset-typescript": "catalog:",
		"@codecov/vite-plugin": "catalog:",
		"@effect/language-service": "catalog:",
		"@mdx-js/rollup": "catalog:",
		"@react-router/dev": "catalog:",
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@tailwindcss/vite": "catalog:",
		"@total-typescript/ts-reset": "catalog:",
		"@types/mdx": "catalog:",
		"@types/node": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitest/coverage-v8": "catalog:",
		"@vitest/eslint-plugin": "catalog:",
		"@vitest/ui": "catalog:",
		"babel-plugin-react-compiler": "catalog:",
		"globals": "catalog:",
		"msw": "catalog:",
		"react-router-devtools": "catalog:",
		"tailwindcss": "catalog:",
		"vite": "catalog:",
		"vite-plugin-babel": "catalog:",
		"vite-tsconfig-paths": "catalog:",
		"vitest": "catalog:"
	},
	"imports": {
		"#root/*": "./*"
	},
	"name": "@repo/app",
	"private": true,
	"scripts": {
		"build": "react-router build",
		"check": "biome check .",
		"check:fix": "biome check --apply .",
		"clean": "rm -rf node_modules build coverage pruned .react-router",
		"dev": "node --env-file=.development.env --experimental-network-inspection ./src/server/server.ts",
		"format": "biome check --vcs-enabled=true --vcs-use-ignore-file=true  --formatter-enabled=true --linter-enabled=false .",
		"format:write": "biome check --vcs-enabled=true --vcs-use-ignore-file=true  --formatter-enabled=true --linter-enabled=false --write .",
		"lint": "biome ci --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=false --linter-enabled=true --no-errors-on-unmatched .",
		"start": "node --env-file=.production.env --inspect ./src/server/server.ts",
		"test": "vitest",
		"typecheck": "react-router typegen && tsc --noEmit"
	},
	"sideEffects": false,
	"type": "module"
}
