# Ignore artifacts:
build
coverage
dist
.turbo

# Ignore all log files:
*.log

# Ignore Node.js modules:
~/
**/node_modules

# Ignore package lock files:
package-lock.json
pnpm-lock.yaml
yarn.lock

# Ignore Git directory:
**/.git

# Ignore IDE specific files:
.idea
.vscode


# Ignore other files that don't need to be formatted:
*.min.js
*.bundle.js

# Ignore TypeScript declaration files:
*.d.ts

# Ignore pnpm specific files and directories:
.pnpm
pnpm-workspace.yaml
pnpm-lock.yaml
