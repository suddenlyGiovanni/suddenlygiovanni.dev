{
	"$schema": "https://json.schemastore.org/package.json",
	"dependencies": {
		"@nick/clsx": "jsr:0.3.1",
		"@radix-ui/react-accessible-icon": "catalog:",
		"@radix-ui/react-accordion": "catalog:",
		"@radix-ui/react-avatar": "catalog:",
		"@radix-ui/react-collapsible": "catalog:",
		"@radix-ui/react-dropdown-menu": "catalog:",
		"@radix-ui/react-icons": "catalog:",
		"@radix-ui/react-navigation-menu": "catalog:",
		"@radix-ui/react-separator": "catalog:",
		"@radix-ui/react-slot": "catalog:",
		"@radix-ui/react-switch": "catalog:",
		"@repo/tailwind-config": "workspace:*",
		"react": "catalog:",
		"react-aria-components": "catalog:",
		"react-dom": "catalog:",
		"react-router": "catalog:",
		"tailwind-variants": "catalog:"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "catalog:",
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@storybook/addon-a11y": "catalog:",
		"@storybook/addon-essentials": "catalog:",
		"@storybook/addon-interactions": "catalog:",
		"@storybook/addon-links": "catalog:",
		"@storybook/addon-onboarding": "catalog:",
		"@storybook/addon-themes": "catalog:",
		"@storybook/blocks": "catalog:",
		"@storybook/react": "catalog:",
		"@storybook/react-vite": "catalog:",
		"@storybook/test": "catalog:",
		"@tailwindcss/vite": "catalog:",
		"@total-typescript/ts-reset": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitejs/plugin-react": "catalog:",
		"babel-plugin-react-compiler": "catalog:",
		"class-variance-authority": "catalog:",
		"eslint-plugin-storybook": "catalog:",
		"globals": "catalog:",
		"storybook": "catalog:",
		"tailwind-merge": "catalog:",
		"tailwindcss": "catalog:",
		"vite": "catalog:",
		"vitest": "catalog:"
	},
	"exports": {
		"./components/*": "./src/components/*",
		"./hooks/*": "./src/hooks/*",
		"./lib/*": "./src/lib/*",
		"./styles.css": "./src/styles/styles.css",
		"./ui/*": "./src/ui/*"
	},
	"imports": {
		"#*": "./src/*"
	},
	"name": "@repo/ui",
	"peerDependencies": {
		"react": "catalog:"
	},
	"private": true,
	"scripts": {
		"build-storybook": "storybook build",
		"check": "biome check .",
		"check:fix": "biome check --apply .",
		"chromatic": "npx chromatic",
		"clean": "rm -rf node_modules storybook-static",
		"components-add": "pnpm dlx shadcn-ui@latest add",
		"components-diff": "pnpm dlx shadcn-ui@latest diff",
		"dev": "storybook dev -p 6006",
		"eslint": "eslint . --max-warnings 0",
		"format": "biome check --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=true --linter-enabled=false .",
		"format:write": "biome check --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=true --linter-enabled=false --write .",
		"lint": "biome ci --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=false --linter-enabled=true --no-errors-on-unmatched .",
		"typecheck": "tsc --noEmit"
	},
	"type": "module"
}
