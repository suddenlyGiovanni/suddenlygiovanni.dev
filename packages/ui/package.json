{
	"$schema": "https://json.schemastore.org/package.json",
	"config": {
		"scripty": {
			"path": "../../scripts"
		}
	},
	"dependencies": {
		"@nick/clsx": "npm:@jsr/nick__clsx@0.3.1",
		"@radix-ui/react-accessible-icon": "1.1.0",
		"@radix-ui/react-accordion": "1.2.1",
		"@radix-ui/react-avatar": "1.1.1",
		"@radix-ui/react-collapsible": "1.1.1",
		"@radix-ui/react-dropdown-menu": "2.1.2",
		"@radix-ui/react-icons": "1.3.2",
		"@radix-ui/react-navigation-menu": "1.2.1",
		"@radix-ui/react-separator": "1.1.0",
		"@radix-ui/react-slot": "1.1.0",
		"@radix-ui/react-switch": "1.1.1",
		"react": "18.3.1",
		"react-aria-components": "1.5.0",
		"react-router": "7.0.2",
		"tailwind-variants": "0.3.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "3.2.2",
		"@storybook/addon-essentials": "8.4.6",
		"@storybook/addon-interactions": "8.4.6",
		"@storybook/addon-links": "8.4.6",
		"@storybook/addon-onboarding": "8.4.6",
		"@storybook/addon-themes": "8.4.6",
		"@storybook/blocks": "8.4.6",
		"@storybook/react": "8.4.6",
		"@storybook/react-vite": "8.4.6",
		"@storybook/test": "8.4.6",
		"@suddenlygiovanni/config-typescript": "workspace:*",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0-beta.5",
		"@total-typescript/ts-reset": "0.6.1",
		"@types/react": "18.3.13",
		"@types/react-dom": "18.3.1",
		"@vitejs/plugin-react-swc": "3.7.2",
		"class-variance-authority": "0.7.1",
		"storybook": "8.4.6",
		"tailwind-merge": "2.5.5",
		"tailwindcss": "^4.0.0-beta.5",
		"tailwindcss-animate": "^1.0.7",
		"vite": "5.4.11",
		"vite-tsconfig-paths": "5.1.3"
	},
	"engines": {
		"node": ">=22.11"
	},
	"exports": {
		"./tailwind.config.ts": "./tailwind.config.ts",
		"./lib/*": "./src/lib/*",
		"./components/*": "./src/components/*",
		"./ui/*": "./src/ui/*",
		"./hooks/*": "./src/hooks/*"
	},
	"name": "@suddenlygiovanni/ui",
	"packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab",
	"peerDependencies": {
		"react": "18.3.1"
	},
	"private": true,
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build-storybook": "storybook build",
		"check": "biome check .",
		"check:fix": "biome check --apply .",
		"chromatic": "npx chromatic",
		"clean": "scripty",
		"components-add": "pnpm dlx shadcn-ui@latest add",
		"components-diff": "pnpm dlx shadcn-ui@latest diff",
		"dev": "storybook dev -p 6006",
		"format": "biome check --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=true .",
		"format:ci": "biome check --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=true --reporter=github --changed .",
		"format:ci:write": "biome check --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=true --reporter=github --changed --write .",
		"format:write": "biome check --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=true --linter-enabled=false --organize-imports-enabled=true --write .",
		"lint": "biome ci --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=false --linter-enabled=true --organize-imports-enabled=false --no-errors-on-unmatched .",
		"lint:ci": "pnpm run lint --reporter=github",
		"lint:ci:write": "biome check --vcs-enabled=true --vcs-use-ignore-file=true --vcs-root='../../' --formatter-enabled=false --linter-enabled=true --organize-imports-enabled=true --no-errors-on-unmatched --reporter=github --changed --write .",
		"typecheck": "scripty"
	},
	"type": "module",
	"version": "0.0.1"
}
