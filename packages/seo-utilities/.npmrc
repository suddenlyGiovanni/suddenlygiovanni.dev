; Lockfile Settings
lockfile = true
prefer-frozen-lockfile = true

; CLI Settings
color = 'auto'
loglevel = 'info'
strict-peer-dependencies = false
use-beta-cli = false
recursive-install = true
engine-strict = true

; Build Settings
child-concurrency = 5
side-effects-cache = false
side-effects-cache-readonly = false
unsafe-perm = false

; Other Settings
use-running-store-server = false
save-prefix = '~'
tag = 'latest'

