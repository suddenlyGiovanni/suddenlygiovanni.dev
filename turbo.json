{
  "$schema": "https://turbo.build/schema.json",
	"tasks": {
    "build": {
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "topo"
      ]
    },
    "format": {
      "cache": false
    },
    "check": {},
    "test:unit": {
      "dependsOn": [
        "topo"
      ]
    },
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "topo"
      ]
    }
  }
}
