{
	"$schema": "https://turbo.build/schema.json",
	"experimentalUI": true,
	"pipeline": {
		"build": {
			"outputs": ["dist/**", "storybook-static/**"],
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["topo"]
		},
		"format": {
			"cache": false
		},
		"lint": {},
		"test:unit": {
			"dependsOn": ["topo"]
		},
		"topo": {
			"dependsOn": ["^topo"]
		},
		"typecheck": {
			"dependsOn": ["topo"]
		}
	}
}
