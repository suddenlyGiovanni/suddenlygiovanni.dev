{
	"name": "@suddenly-giovanni/web",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"check": "biome check .",
		"check:fix": "biome check --apply .",
		"clean": "scripty",
		"dev": "remix vite:dev --profile",
		"format": "scripty",
		"inspect-router": "remix routes | bat --language TypeScriptReact",
		"lint": "scripty",
		"start": "remix-serve ./build/server/index.js",
		"test": "vitest",
		"test:unit": "vitest run --coverage",
		"typecheck": "scripty"
	},
	"dependencies": {
		"@effect/schema": "0.64.13",
		"@epic-web/client-hints": "1.3.0",
		"@epic-web/invariant": "1.0.0",
		"@remix-run/node": "2.8.1",
		"@remix-run/react": "2.8.1",
		"@remix-run/serve": "2.8.1",
		"@suddenly-giovanni/resume": "workspace:*",
		"@suddenly-giovanni/ui": "workspace:*",
		"cookie": "0.6.0",
		"effect": "2.4.13",
		"isbot": "5.1.2",
		"react": "canary",
		"react-dom": "canary"
	},
	"devDependencies": {
		"@codecov/vite-plugin": "0.0.1-beta.5",
		"@mdx-js/rollup": "3.0.1",
		"@remix-run/dev": "2.8.1",
		"@remix-run/testing": "2.8.1",
		"@suddenly-giovanni/config-prettier": "workspace:*",
		"@suddenly-giovanni/config-tailwind": "workspace:*",
		"@suddenly-giovanni/config-typescript": "workspace:*",
		"@suddenly-giovanni/eslint-config": "workspace:*",
		"@total-typescript/ts-reset": "0.5.1",
		"@types/cookie": "0.6.0",
		"@types/react": "18.2.70",
		"@types/react-dom": "18.2.22",
		"@typescript-eslint/eslint-plugin": "7.3.1",
		"@vitest/coverage-v8": "1.4.0",
		"@vitest/ui": "1.4.0",
		"autoprefixer": "10.4.19",
		"eslint-config-prettier": "9.1.0",
		"eslint-import-resolver-typescript": "3.6.1",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-react": "7.34.1",
		"eslint-plugin-react-hooks": "4.6.0",
		"postcss": "8.4.38",
		"remark-frontmatter": "5.0.0",
		"remark-mdx-frontmatter": "4.0.0",
		"remix-development-tools": "4.0.10",
		"tailwindcss": "3.4.1",
		"vite": "5.2.6",
		"vite-tsconfig-paths": "4.3.2",
		"vitest": "1.4.0"
	},
	"engines": {
		"node": ">=21"
	},
	"config": {
		"scripty": {
			"path": "../../scripts"
		}
	}
}
