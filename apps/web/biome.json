{
	"$schema": "https://biomejs.dev/schemas/1.6.3/schema.json",
	"extends": ["../../biome.json"],
	"files": {
		"include": ["app/*"],
		"ignoreUnknown": true,
		"ignore": ["./~"]
	},
	"overrides": [
		{
			"include": ["app/routes/**/*.tsx"],
			"linter": {
				"rules": {
					"style": {
						"noDefaultExport": "off"
					}
				}
			}
		}
	]
}
