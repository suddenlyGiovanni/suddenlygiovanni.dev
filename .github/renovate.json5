{
	$schema: 'https://docs.renovatebot.com/renovate-schema.json',
	configMigration: true,
	dockerfile: {
		enabled: true,
	},
	extends: [
		'config:recommended',
		':pinDevDependencies',
		'group:monorepos',
		'group:recommended',
		'replacements:all',
		'workarounds:all',
		':followTag(typescript, rc)',
		'schedule:earlyMondays',
	],
	npmrcMerge: true,
	hostRules: [
		{
			matchHost: 'https://npm.pkg.github.com/',
			hostType: 'npm',
			encrypted: {
				token: '',
			},
		},
	],
	labels: [
		'dependencies',
	],
	packageRules: [
		{
			groupName: 'effect packages',
			matchPackageNames: [
				'/^effect$/',
				'/^@effect//',
			],
		},
	],
}
