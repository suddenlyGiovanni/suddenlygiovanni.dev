{
	$schema: 'https://docs.renovatebot.com/renovate-schema.json',
	configMigration: true,
	extends: [
		'config:recommended',
		':pinDevDependencies',
		'group:monorepos',
		'group:recommended',
		'replacements:all',
		'workarounds:all',
		':followTag(typescript, rc)',
		'schedule:earlyMondays'
	],
	labels: [
		'dependencies'
	],
	dockerfile: {
		enabled: true
	},
	hostRules: [
		{
			"matchHost": "https://npm.pkg.github.com/",
			"hostType": "npm",
			"encrypted": {
				"token": ""
			}
		}
	],
	packageRules: [
		{
			matchPackagePatterns: [
				'^effect$',
				'^@effect/'
			],
			groupName: 'effect packages'
		}
	]
}
